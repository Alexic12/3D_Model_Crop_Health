{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "S3FullAccess",
			"Effect": "Allow",
			"Action": "s3:*",
			"Resource": "*"
		},
		{
			"Sid": "LambdaFullAccess",
			"Effect": "Allow",
			"Action": "lambda:*",
			"Resource": "*"
		},
		{
			"Sid": "CloudWatchLogsAccess",
			"Effect": "Allow",
			"Action": [
				"logs:*",
				"cloudwatch:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "DynamoDBFullAccess",
			"Effect": "Allow",
			"Action": "dynamodb:*",
			"Resource": "*"
		},
		{
			"Sid": "ApiGatewayFullAccess",
			"Effect": "Allow",
			"Action": [
				"apigateway:*",
				"execute-api:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "LightsailFullAccess",
			"Effect": "Allow",
			"Action": "lightsail:*",
			"Resource": "*"
		},
		{
			"Sid": "IAMSelfManagement",
			"Effect": "Allow",
			"Action": [
				"iam:GetUser",
				"iam:ListUserPolicies",
				"iam:ListAttachedUserPolicies",
				"iam:ListGroupsForUser",
				"iam:CreateAccessKey",
				"iam:DeleteAccessKey",
				"iam:ListAccessKeys",
				"iam:UpdateAccessKey",
				"iam:CreateServiceLinkedRole"
			],
			"Resource": "arn:aws:iam::*:user/${aws:username}"
		},
		{
			"Sid": "IAMRolePolicyCreation",
			"Effect": "Allow",
			"Action": [
				"iam:CreateRole",
				"iam:PutRolePolicy",
				"iam:AttachRolePolicy",
				"iam:CreatePolicy",
				"iam:GetRole",
				"iam:GetPolicy",
				"iam:PassRole"
			],
			"Resource": "*"
		},
		{
			"Sid": "CloudFormationAccess",
			"Effect": "Allow",
			"Action": "cloudformation:*",
			"Resource": "*"
		},
		{
			"Sid": "SageMakerFullAccess",
			"Effect": "Allow",
			"Action": [
				"sagemaker:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "SecretsManagerAccess",
			"Effect": "Allow",
			"Action": "secretsmanager:*",
			"Resource": "*"
		},
		{
			"Sid": "SSMAccess",
			"Effect": "Allow",
			"Action": [
				"ssm:*",
				"ssm-sap:*",
				"ec2messages:*",
				"ssmmessages:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ECRFullAccess",
			"Effect": "Allow",
			"Action": "ecr:*",
			"Resource": "*"
		},
		{
			"Sid": "EventBridgeFullAccess",
			"Effect": "Allow",
			"Action": "events:*",
			"Resource": "*"
		},
		{
			"Sid": "EC2FullAccess",
			"Effect": "Allow",
			"Action": "ec2:*",
			"Resource": "*"
		},
		{
			"Sid": "ELBFullAccess",
			"Effect": "Allow",
			"Action": [
				"elasticloadbalancing:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "WAFFullAccess",
			"Effect": "Allow",
			"Action": [
				"waf:*",
				"wafv2:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ECSFullAccess",
			"Effect": "Allow",
			"Action": [
				"ecs:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "IAMFullAccess",
			"Effect": "Allow",
			"Action": [
				"iam:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "S3AccessAgain",
			"Effect": "Allow",
			"Action": [
				"s3:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "AccessAnalyzerFullAccess",
			"Effect": "Allow",
			"Action": [
				"access-analyzer:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ECRAccessAgain",
			"Effect": "Allow",
			"Action": [
				"ecr:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ECRDescribeOnly",
			"Effect": "Allow",
			"Action": [
				"ecr:DescribeRepositories"
			],
			"Resource": "*"
		},
		{
			"Sid": "AllowECRFullAccess",
			"Effect": "Allow",
			"Action": [
				"ecr:CreateRepository",
				"ecr:DescribeRepositories",
				"ecr:GetAuthorizationToken",
				"ecr:PutImage",
				"ecr:InitiateLayerUpload",
				"ecr:UploadLayerPart",
				"ecr:CompleteLayerUpload",
				"ecr:BatchCheckLayerAvailability"
			],
			"Resource": "*"
		}
	]
}